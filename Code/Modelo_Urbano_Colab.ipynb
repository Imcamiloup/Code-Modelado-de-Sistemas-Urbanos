    "# Cálculo de indicadores para agua\n",
    "def indicadores_agua(solW, tspan, pars):\n",
    "    W = solW.y[0]\n",
    "    P_vec = np.array([P(t) for t in tspan])\n",
    "    W_pc = W / np.maximum(P_vec, 1e-9)\n",
    "    comp = [components_agua(t, W[i], pars) for i,t in enumerate(tspan)]\n",
    "    R_, Qin_, _, _, _, Dtot_, Efl_, _ = map(lambda k: np.array([c[k] for c in comp]), range(8))\n",
    "    oferta_neta = R_ + Qin_ - Efl_\n",
    "    deficit = np.maximum(0.0, Dtot_ - oferta_neta)\n",
    "    confiab = np.mean(deficit <= 1e-9)\n",
    "    return W_pc, deficit, confiab, oferta_neta, Dtot_\n",
